N = zeros(1000);
N(562:564,890:893) = [9 6 2 1;2 4 2 2;9 3 7 8];
[n,c] = size(N);
disp(['N tiene' num2str(n) 'lineas y' num2str(c) 'columnas']);
disp(N(561:565,889:894))
P = sparse(N);
disp(P)

